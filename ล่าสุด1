local es = w:Tab({Title="ESP"})
es:Section({Title="ESP Box/Line/Distance / สิงมุมมอง"})
local eb,el,ed=false,false,false
es:Toggle({Title="Box",Callback=function(v) if not expired then eb=v end end})
es:Toggle({Title="Line",Callback=function(v) if not expired then el=v end end})
es:Toggle({Title="Distance",Callback=function(v) if not expired then ed=v end end})

local targetViewPlayer = nil
local isPossessing = false
local originalCameraType = nil
local originalCameraSubject = nil

es:Section({Title="สิงมุมมอง"})
local playerList = {}
local selectedPlayerName = nil
local function UpdatePlayerList()
    playerList = {}
    for _,pp in pairs(Players:GetPlayers()) do
        if pp ~= Player then table.insert(playerList, pp.Name) end
    end
end
UpdatePlayerList()
Players.PlayerAdded:Connect(UpdatePlayerList)
Players.PlayerRemoving:Connect(function(p) UpdatePlayerList() if selectedPlayerName == p.Name then selectedPlayerName = nil end end)

es:Dropdown({Title="เลือกผู้เล่นเพื่อสิง",List=playerList,Callback=function(name) selectedPlayerName = name end})
es:Button({Title="สิงมุมมองผู้เล่นที่เลือก",Callback=function()
    if expired then return end
    if not selectedPlayerName then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="ยังไม่ได้เลือกผู้เล่น",Duration=3}) return end
    local pp = Players:FindFirstChild(selectedPlayerName)
    if not pp or not pp.Character or not pp.Character:FindFirstChild("HumanoidRootPart") then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="ผู้เล่นไม่พร้อม",Duration=3}) return end

    if isPossessing then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="กำลังสิงอยู่แล้ว",Duration=3}) return end
   
    originalCameraType = Workspace.CurrentCamera.CameraType
    originalCameraSubject = Workspace.CurrentCamera.CameraSubject
    isPossessing = true
    targetViewPlayer = pp

    Workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
    local rsConn
    rsConn = RunService.RenderStepped:Connect(function()
        if not isPossessing or not targetViewPlayer or not targetViewPlayer.Character or not targetViewPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if rsConn then rsConn:Disconnect() end
            return
        end
        local root = targetViewPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root then
            
            local cf = root.CFrame * CFrame.new(0, 1.5, -2) * CFrame.Angles(-0.1, 0, 0)
            Workspace.CurrentCamera.CFrame = cf
        end
    end)
    StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="เริ่มสิงมุมมอง: "..selectedPlayerName,Duration=3})
end})

es:Button({Title="ยกเลิกการสิง",Callback=function()
    if not isPossessing then StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="ไม่ได้สิงอยู่",Duration=3}) return end
    isPossessing = false
    targetViewPlayer = nil
  
    if originalCameraType then Workspace.CurrentCamera.CameraType = originalCameraType end
    if originalCameraSubject and originalCameraSubject.Parent then Workspace.CurrentCamera.CameraSubject = originalCameraSubject end
    StarterGui:SetCore("SendNotification",{Title="Mos Hub",Text="ยกเลิกการสิงแล้ว",Duration=3})
end})

local DS = {}
local function ClearESP(plr)
    if DS[plr] then
        for _,o in pairs(DS[plr]) do
            if type(o.Remove) == "function" then
                pcall(function() o:Remove() end)
            end
        end
    end
    DS[plr] = nil
end
Players.PlayerRemoving:Connect(function(plr) ClearESP(plr) end)

RunService.RenderStepped:Connect(function()
    if expired then
        for _,d in pairs(DS) do for _,o in pairs(d) do if o and type(o.Visible)~="nil" then o.Visible=false end end end
        return
    end
    if not Player.Character or not Player.Character:FindFirstChild("HumanoidRootPart") then return end
    local cam = Workspace.CurrentCamera
    for _,pp in pairs(Players:GetPlayers()) do
        if pp~=Player and pp.Character and pp.Character:FindFirstChild("HumanoidRootPart") then
            local root = pp.Character.HumanoidRootPart
            local screenPos, onScreen = cam:WorldToViewportPoint(root.Position)
            local dist = (Player.Character.HumanoidRootPart.Position - root.Position).Magnitude
            if not DS[pp] then DS[pp] = {} end
            
            if eb then
                if not DS[pp].B then local b=Drawing.new("Square"); b.Thickness=1; b.Color=Color3.fromRGB(255,0,0); b.Filled=false; DS[pp].B=b end
                DS[pp].B.Size = Vector2.new(50,50)
                DS[pp].B.Position = Vector2.new(screenPos.X-25, screenPos.Y-25)
                DS[pp].B.Visible = onScreen
            elseif DS[pp].B then DS[pp].B.Visible=false end
            
            if el then
                if not DS[pp].L then local l=Drawing.new("Line"); l.Color=Color3.fromRGB(0,255,0); l.Thickness=1; DS[pp].L=l end
                DS[pp].L.From = Vector2.new(cam.ViewportSize.X/2, cam.ViewportSize.Y/2)
                DS[pp].L.To = Vector2.new(screenPos.X, screenPos.Y)
                DS[pp].L.Visible = onScreen
            elseif DS[pp].L then DS[pp].L.Visible=false end
            
            if ed then
                if not DS[pp].D then local t=Drawing.new("Text"); t.Color=Color3.fromRGB(255,255,0); t.Size=14; t.Center=true; t.Outline=true; DS[pp].D=t end
                DS[pp].D.Text = string.format("%.1f", dist)
                DS[pp].D.Position = Vector2.new(screenPos.X, screenPos.Y-35)
                DS[pp].D.Visible = onScreen
            elseif DS[pp].D then DS[pp].D.Visible=false end
        end
    end
end)

local wp = w:Tab({Title="วาป"})
wp:Section({Title="วาปไปผู้เล่น"})
local Wlist = {}
local selectedPlayer = nil
local function UpdateWlist()
    Wlist = {}
    for _,pp in pairs(Players:GetPlayers()) do
        if pp~=Player then table.insert(Wlist, pp.Name) end
    end
end
UpdateWlist()
Players.PlayerAdded:Connect(UpdateWlist)
Players.PlayerRemoving:Connect(UpdateWlist)
wp:Dropdown({Title="เลือกผู้เล่น",List=Wlist,Callback=function(n) selectedPlayer = Players:FindFirstChild(n) end})
wp:Button({Title="ยืนยันวาป",Callback=function()
    if expired or not selectedPlayer then return end
    if Player.Character and selectedPlayer.Character and selectedPlayer.Character:FindFirstChild("HumanoidRootPart") then
        Player.Character.HumanoidRootPart.CFrame = selectedPlayer.Character.HumanoidRootPart.CFrame
    end
end})

local inv = w:Tab({Title="ล่องหน"})
inv:Section({Title="ล่องหน"})
inv:Toggle({Title="ล่องหน",Callback=function(v)
    if expired then return end
    if Player.Character then
        for _,o in pairs(Player.Character:GetDescendants()) do
            if o:IsA("BasePart") and o.Name~="HumanoidRootPart" then
                o.Transparency = v and 1 or 0
                o.CanCollide = not v
            end
        end
    end
end})


local fly = w:Tab({Title="บิน"})
fly:Button({Title="เปิดระบบบิน",Callback=function()
    rv(function()
        loadstring("\108\111\97\100\115\116\114\105\110\103\40\103\97\109\101\58\72\116\116\112\71\101\116\40\40\39\104\116\116\112\115\58\47\47\103\105\115\116\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\109\101\111\122\111\110\101\89\84\47\98\102\48\51\55\100\102\102\57\102\48\97\55\48\48\49\55\51\48\52\100\100\100\54\55\102\100\99\100\51\55\48\47\114\97\119\47\101\49\52\101\55\52\102\52\50\53\98\48\54\48\100\102\53\50\51\51\52\51\99\102\51\48\98\55\56\55\48\55\52\101\98\51\99\53\100\50\47\97\114\99\101\117\115\37\50\53\50\48\120\37\50\53\50\48\102\108\121\37\50\53\50\48\50\37\50\53\50\48\111\98\102\108\117\99\97\116\111\114\39\41\44\116\114\117\101\41\41\40\41\10\10")()


    end)
end})

local tt = w:Tab({Title="เวลาสคริปต์"})
tt:Section({Title="หมดอายุ"})
local lab = tt:Label({Title="หมดอายุวันที่ 19/10/2025"})
tt:Label({Title="วันที่เริ่ม: "..os.date("%d/%m/%Y %H:%M:%S",startTime)})

spawn(function()
    while true do
        local rem = expireTime - os.time()
        if rem<=0 then
            lab:Set("⚠️ สคริปต์หมดอายุแล้ว ⚠️")
            if not expired then
                expired=true
               
                pcall(function() w:Close() end)
                fpsText.Visible = false
               
                isPossessing = false
                targetViewPlayer = nil
                if originalCameraType then Workspace.CurrentCamera.CameraType = originalCameraType end
                if originalCameraSubject and originalCameraSubject.Parent then pcall(function() Workspace.CurrentCamera.CameraSubject = originalCameraSubject end) end
              
                showExpiredScreen()
            end
        else
            local d = math.floor(rem/86400)
            local h = math.floor((rem%86400)/3600)
            local m = math.floor((rem%3600)/60)
            local s = rem%60
            lab:Set(string.format("เวลาที่เหลือ: %02d วัน %02d:%02d:%02d", d, h, m, s))
        end
        task.wait(1)
    end
end)
end


showUltimateGUI(initMainHub)
