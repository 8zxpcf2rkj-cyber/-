local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local StarterGui = game:GetService("StarterGui")
local Lighting = game:GetService("Lighting")
local HttpService = game:GetService("HttpService")
local TweenService = game:GetService("TweenService")

local titleText = "MOS HUB"
local letterDelay = 0.28
local letterTweenTime = 0.35
local panelSize = UDim2.new(0, 900, 0, 330)
local panelPos = UDim2.new(0.5, -450, 0.5, -165)
local mainAccent = Color3.fromRGB(150, 0, 255) 
local glowColor = Color3.fromRGB(200, 100, 255)
local pulseLightSpeed = 3
local powerEffectSpeed = 0.015
local voiceId = "rbxassetid://YOUR_BANG_MOSS_SOUND_ID" 
local smokeTexture = "rbxassetid://258128463"

local old = Player:FindFirstChild("MosHubGui")
if old then old:Destroy() end

local function showUltimateGUI(callback)
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "MosHubGui"
    screenGui.ResetOnSpawn = false
    screenGui.IgnoreGuiInset = true
    screenGui.Parent = Player:WaitForChild("PlayerGui")

    local panel = Instance.new("Frame")
    panel.Name = "MainPanel"
    panel.Parent = screenGui
    panel.Size = panelSize
    panel.Position = panelPos
    panel.BackgroundColor3 = Color3.fromRGB(10, 10, 20)
    panel.BackgroundTransparency = 0.55
    panel.BorderSizePixel = 0
    Instance.new("UICorner", panel).CornerRadius = UDim.new(0, 25)

    local stroke = Instance.new("UIStroke")
    stroke.Color = mainAccent
    stroke.Thickness = 2
    stroke.Transparency = 0.5
    stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    stroke.Parent = panel

    local grad = Instance.new("UIGradient")
    grad.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(15,10,25)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(35,25,45))
    }
    grad.Rotation = 90
    grad.Parent = panel

 
    local smoke = Instance.new("ImageLabel")
    smoke.BackgroundTransparency = 1
    smoke.Image = smokeTexture
    smoke.ImageColor3 = mainAccent
    smoke.ImageTransparency = 0.8
    smoke.Size = UDim2.new(2, 0, 2, 0)
    smoke.Position = UDim2.new(-0.5, 0, -0.5, 0)
    smoke.ZIndex = 0
    smoke.Parent = panel

    task.spawn(function()
        while smoke.Parent do
            smoke.Rotation = (smoke.Rotation + 0.2) % 360
            task.wait(0.03)
        end
    end)

    local titleFrame = Instance.new("Frame")
    titleFrame.Size = UDim2.new(1, -80, 1, -80)
    titleFrame.Position = UDim2.new(0, 40, 0, 40)
    titleFrame.BackgroundTransparency = 1
    titleFrame.Parent = panel

    local layout = Instance.new("UIListLayout")
    layout.FillDirection = Enum.FillDirection.Horizontal
    layout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    layout.VerticalAlignment = Enum.VerticalAlignment.Center
    layout.Padding = UDim.new(0, 10)
    layout.Parent = titleFrame

    local beep = Instance.new("Sound")
    beep.SoundId = "rbxassetid://9118823104"
    beep.Volume = 0.4
    beep.Parent = panel

    local voice = Instance.new("Sound")
    voice.SoundId = voiceId
    voice.Volume = 1
    voice.PlaybackSpeed = 0.82
    voice.Parent = panel

    local letters = {}
    for i = 1, #titleText do
        local ch = titleText:sub(i,i)
        local lbl = Instance.new("TextLabel")
        lbl.Size = UDim2.new(0, 110, 0, 130)
        lbl.BackgroundTransparency = 1
        lbl.Text = ch
        lbl.Font = Enum.Font.GothamBlack
        lbl.TextSize = 110
        lbl.TextColor3 = Color3.fromRGB(200,200,255)
        lbl.TextTransparency = 1
        lbl.Parent = titleFrame

        local glow = Instance.new("UIStroke")
        glow.Color = glowColor
        glow.Thickness = 2
        glow.Transparency = 0.8
        glow.Parent = lbl

        table.insert(letters,lbl)
    end

    
    task.spawn(function()
        while panel.Parent do
            local hue = (tick() * pulseLightSpeed) % 1
            stroke.Color = Color3.fromHSV(hue,1,1)
            task.wait(0.03)
        end
    end)

    
    task.spawn(function()
        for _,lbl in pairs(letters) do
            TweenService:Create(lbl, TweenInfo.new(letterTweenTime, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {TextTransparency=0}):Play()
            beep:Play()
            task.wait(letterDelay)
        end
    end)

   
    voice:Play()

   
    local totalDelay = letterDelay * #titleText + voice.TimeLength + 0.5
    task.delay(totalDelay,function()
        if callback then callback() end
        screenGui:Destroy()
    end)
end

local function initMainHub()
   local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local StarterGui = game:GetService("StarterGui")
local Lighting = game:GetService("Lighting")
local HttpService = game:GetService("HttpService")

local function parseTime(str)
	local y,m,d,H,M,S = string.match(str,"(%d+)-(%d+)-(%d+) (%d+):(%d+):(%d+)")
	return os.time({year=tonumber(y),month=tonumber(m),day=tonumber(d),hour=tonumber(H),min=tonumber(M),sec=tonumber(S)})
end
local expireTime = parseTime("2025-10-19 23:59:59") 
local startTime = os.time()
local expired = false
local function valid() return os.time() <= expireTime and not expired end
local function rv(f)
    if valid() then f() 
    else StarterGui:SetCore("SendNotification",{Title="สคริปต์ปิดอัปเดต",Text="สคริปต์หมดอายุแล้ว",Duration=5}) 
    end
end

local DISCORD_LINK = "https://discord.gg/qpsEcM3j" 

local function showExpiredScreen()
    local gui = Instance.new("ScreenGui")
    gui.Name = "MosHub_Expired"
    gui.ResetOnSpawn = false
    gui.Parent = Player:WaitForChild("PlayerGui")

    local bg = Instance.new("Frame", gui)
    bg.Size = UDim2.new(1,0,1,0)
    bg.Position = UDim2.new(0,0,0,0)
    bg.BackgroundTransparency = 1

    local label = Instance.new("TextLabel", bg)
    label.Size = UDim2.new(1,0,0.2,0)
    label.Position = UDim2.new(0,0,0.35,0)
    label.BackgroundTransparency = 1
    label.Text = "⚠️ รอสคริปต์อัปเดต ⚠️"
    label.TextScaled = true
    label.TextColor3 = Color3.fromRGB(255,50,50)
    label.Font = Enum.Font.SourceSansBold

    local btn = Instance.new("TextButton", bg)
    btn.Size = UDim2.new(0,300,0,50)
    btn.Position = UDim2.new(0.5,-150,0.6,0)
    btn.Text = "คัดลอกลิงก์ Discord"
    btn.TextScaled = true
    btn.Font = Enum.Font.SourceSansBold
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = Color3.fromRGB(30,144,255)
    btn.AutoButtonColor = true

    btn.MouseButton1Click:Connect(function()
        if setclipboard then
            pcall(function() setclipboard(DISCORD_LINK) end)
            StarterGui:SetCore("SendNotification",{Title="คัดลอกแล้ว",Text="ลิงก์ Discord ถูกคัดลอกแล้ว",Duration=4})
        else
            StarterGui:SetCore("SendNotification",{Title="ไม่สามารถคัดลอกได้",Text="Executor ของคุณไม่รองรับ setclipboard",Duration=4})
        end
    end)
end

if os.time() > expireTime then
    showExpiredScreen()
    return
end

local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/x2zu/OPEN-SOURCE-UI-ROBLOX/refs/heads/main/X2ZU%20UI%20ROBLOX%20OPEN%20SOURCE/DummyUi-leak-by-x2zu/fetching-main/Tools/Framework.luau"))()
local w = lib:Window({
    Title="Mos Hub | by บังมอส น้ะจ้ะ",
    Desc="Mos Hub on top",
    Icon=105059922903197,
    Theme="Dark",
    Config={Keybind=Enum.KeyCode.P,Size=UDim2.new(0,700,0,520)},
    CloseUIButton={Enabled=true,Text="Mos Hub"}
})

local fpsText = Drawing.new("Text")
fpsText.Text = "FPS: 0"; fpsText.Size = 22; fpsText.Color = Color3.fromRGB(0,255,0); fpsText.Center = true; fpsText.Outline = true; fpsText.Visible = true
local dragging, mousePos, fpsPos = false, Vector2.new(), Vector2.new(150,50)
UserInputService.InputBegan:Connect(function(i)
    if expired then return end
    if i.UserInputType == Enum.UserInputType.MouseButton1 then
        local m = UserInputService:GetMouseLocation()
        if (Vector2.new(m.X,m.Y)-fpsPos).Magnitude <= 50 then
            dragging = true
            mousePos = Vector2.new(m.X,m.Y)-fpsPos
        end
    end
end)
UserInputService.InputEnded:Connect(function(i) if i.UserInputType == Enum.UserInputType.MouseButton1 then dragging=false end end)
UserInputService.InputChanged:Connect(function(i)
    if expired then return end
    if dragging and i.UserInputType == Enum.UserInputType.MouseMovement then
        local m = UserInputService:GetMouseLocation()
        fpsPos = Vector2.new(m.X,m.Y)-mousePos
    end
end)
local lastTick = tick()
RunService.RenderStepped:Connect(function()
    if expired then fpsText.Visible=false return end
    local now = tick()
    local fps = math.floor(1 / math.max((now-lastTick), 0.000001))
    lastTick = now
    fpsText.Text = "FPS: "..fps
    fpsText.Position = fpsPos
end)

local admin = w:Tab({Title="ผู้เล่น"})
admin:Section({Title="ฟังก์ชันแอดมิน"})
local infiniteJump, noclip = false, false

admin:Toggle({Title="กระโดดไม่จำกัด",Callback=function(v) if not expired then infiniteJump=v end end})
admin:Slider({Title="กระโดดสูง",Min=0,Max=500,Value=50,Callback=function(v) if not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.JumpPower=v end end})
admin:Slider({Title="วิ่งไว",Min=0,Max=500,Value=16,Callback=function(v) if not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then Player.Character.Humanoid.WalkSpeed=v end end})
admin:Toggle({Title="เดินทะลุสิ่งของ (Noclip)",Callback=function(v) if not expired then noclip=v end end})

RunService.Stepped:Connect(function()
    if noclip and not expired and Player.Character then
        for _,p in pairs(Player.Character:GetDescendants()) do
            if p:IsA("BasePart") then p.CanCollide=false end
        end
    end
end)

UserInputService.JumpRequest:Connect(function()
    if infiniteJump and not expired and Player.Character and Player.Character:FindFirstChild("Humanoid") then
        Player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

admin:Button({Title="Boot FPS (ลดภาพเพื่อเพิ่มFPS)",Callback=function()
    if expired then return end
    Lighting.GlobalShadows = false
    Lighting.Brightness = 2
    Lighting.FogEnd = 1000
    Lighting.OutdoorAmbient = Color3.fromRGB(128,128,128)
    StarterGui:SetCore("SendNotification", {Title="Mos Hub", Text="Boot FPS สำเร็จ! ภาพกากลงแล้ว", Duration=4})
end})

admin:Button({Title="รัน Infinite Yield",Callback=function() rv(function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end) end})

